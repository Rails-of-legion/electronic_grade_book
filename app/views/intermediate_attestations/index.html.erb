<h1>List of Intermediate Attestations</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Date</th>
      <th>Assessment Type</th>
      <th>Subject</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @intermediate_attestations.each do |intermediate_attestation| %>
      <tr>
        <td>
          <%= intermediate_attestation.name %>
        </td>
        <td>
          <%= intermediate_attestation.date %>
        </td>
        <td>
          <%= intermediate_attestation.assessment_type %>
        </td>
        <td>
          <%= intermediate_attestation.subject.try(:name) %>
        </td>
        <td>
          <%= link_to "Show", intermediate_attestation_path(intermediate_attestation), class: "btn btn-primary" %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
