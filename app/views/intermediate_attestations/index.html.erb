<table class="table table-striped">
  <thead>
    <tr>
      <th class="col-md-3">Name</th>
      <th class="col-md-2">Date</th>
      <th class="col-md-2">Assessment Type</th>
      <th class="col-md-3">Subject</th>
    </tr>
  </thead>
  <tbody>
    <% @intermediate_attestations.each do |intermediate_attestation| %>
      <tr>
        <td>
          <%= link_to intermediate_attestation.name, intermediate_attestation_path(intermediate_attestation) %>
        </td>
        <td>
          <%= intermediate_attestation.date %>
        </td>
        <td>
          <%= intermediate_attestation.assessment_type %>
        </td>
        <td>
          <%= intermediate_attestation.subject.try(:name) %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>