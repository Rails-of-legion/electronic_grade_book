<h1>List of Groups</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Curator</th>
    </tr>
  </thead>
  <tbody>
    <% @groups.each do |group| %>
      <tr>
        <td><%= link_to group.name, group_path(group), class: "btn btn-primary" %></td>
      </tr>
    <% end %>
  </tbody>
</table>
